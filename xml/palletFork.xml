<?xml version="1.0" encoding="utf-8" standalone="no"?>
<vehicle type="implementDynamicMountAttacher" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://validation.gdn.giants-software.com/xml/fs22/vehicle.xsd">
    <annotation>Copyright (C) GIANTS Software GmbH, All Rights Reserved.</annotation>

    <storeData>
        <name>FEM DK edit</name>	
        <specs>
            <combination xmlFilename="xml/MC.xml"/>	
        </specs>
        <functions>
            <function>$l10n_function_Gabelzinken</function>
        </functions>
        <image>icons/store_palletFork.png</image>
        <price>870</price>
        <lifetime>600</lifetime>
        <rotation>0</rotation>
        <brand>MANITOU</brand>
        <category>DennisKtools</category>
        <vertexBufferMemoryUsage>46336</vertexBufferMemoryUsage>
        <indexBufferMemoryUsage>7168</indexBufferMemoryUsage>
        <textureMemoryUsage>524288</textureMemoryUsage>
        <audioMemoryUsage>0</audioMemoryUsage>
        <instanceVertexBufferMemoryUsage>0</instanceVertexBufferMemoryUsage>
        <instanceIndexBufferMemoryUsage>0</instanceIndexBufferMemoryUsage>
    </storeData>

    <base>
        <typeDesc>$l10n_typeDesc_frontloaderTool</typeDesc>
        <filename>i3d/palletFork.i3d</filename>
        <size width="1.5" length="2"/>
        <components>
            <component centerOfMass="0.00 0.18 0.16" solverIterationCount="10" mass="180"/>
        </components>
        <schemaOverlay name="IMPLEMENT" attacherJointPosition="0 0"/>
        <supportsPickUp>false</supportsPickUp>
    </base>

    <attachable>
        <inputAttacherJoints>
            <inputAttacherJoint node="attacherJoint" jointType="implement" hardAttach="false">
                <distanceToGround lower="0.1" upper="1.15"/>
            </inputAttacherJoint>
        </inputAttacherJoints>
    </attachable>

    <animations>
        <animation name="mainArmMovement">
            <part endTime="0.5" endTrans="0.42 0 0" node="Fork_left" startTime="0" startTrans="0 0 0"/>
            <part endTime="0.5" endTrans="-0.42 0 0" node="Fork_right" startTime="0" startTrans="0 0 0"/>
        </animation>
    </animations>

    <cylindered>
        <movingTools>
            <movingTool node="Fork_left" playSound="true">
                <animation animAcceleration="5" animName="mainArmMovement" animSpeed="0.2"/>
                <controls iconName="TOOL_OPEN_CLOSE" axis="AXIS_FRONTLOADER_TOOL2" invertAxis="true" mouseSpeedFactor="0.5"/>
            </movingTool>
        </movingTools>
        <sounds>
            <actionSound template="hydraulicLayerTone2" pitchScale="2.3" volumeScale="0.75" 	linkNode="Fork_left" actionNames="TOOL_MOVE_POS TOOL_MOVE_NEG" nodes="Fork_left" fadeIn="0.1" fadeOut="0.05"/>
        </sounds>
    </cylindered>
	
    <dynamicMountAttacher node="jointNode"/>	
	
    <foliageBending>
        <bendingNode minX="-0.06" maxX="0.06" minZ="-0.77" maxZ="0.47" yOffset="0" node="tineCollisionLeft" />
        <bendingNode minX="-0.06" maxX="0.06" minZ="-0.77" maxZ="0.47" yOffset="0" node="tineCollisionRight" />
    </foliageBending>	

    <wearable wearDuration="480" workMultiplier="5" fieldMultiplier="2"/>
    <washable dirtDuration="90" washDuration="1" workMultiplier="2" fieldMultiplier="2"/>
	
    <i3dMappings>
        <i3dMapping id="paladinPalletFork_main_component1" node="0>"/>
        <i3dMapping id="paladinPalletFork_vis" node="0>0"/>
        <i3dMapping id="attacherJoint" node="0>0|0"/>
        <i3dMapping id="Fork_left" node="0>0|2"/>
        <i3dMapping id="Fork_right" node="0>0|3"/>		
        <i3dMapping id="tineCollisionLeft" node="0>0|2|0|0" />
        <i3dMapping id="dynamicMountTriggerLeft" node="0>0|2|0|1" />		
        <i3dMapping id="tineCollisionRight" node="0>0|3|0|0" />	
        <i3dMapping id="dynamicMountTriggerRight" node="0>0|3|0|1" />
        <i3dMapping id="jointNode" node="0>1" />	
    </i3dMappings>
</vehicle>
